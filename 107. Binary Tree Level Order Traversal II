# Definition for a binary tree node.
# class TreeNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution(object):
    def levelOrderBottom(self, root):
        """
        :type root: TreeNode
        :rtype: List[List[int]]
        """
        result = []
        self.add_level(root, 0, result)
        return result
        
    def add_level(self, root, level, result):
        if not root:
            return
        elif len(result)<=level:
            result.insert(0,[root.val])
        else: result[len(result)-level-1].append(root.val)
        
        self.add_level(root.left, level+1,result)
        self.add_level(root.right,level+1,result)
