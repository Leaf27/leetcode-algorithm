class Solution(object):
    def countPrimes(self, n):
        """
        :type n: int
        :rtype: int
        """
        if (n<=2):
            return 0
        status= [True]*(n)
        status[0]=status[1]=False
        for i in range(2,int((n-1)*0.5)+1):
            if status[i]:
                status[i*i:n:i]=[False]*len(status[i*i:n:i])
        return sum(status)
