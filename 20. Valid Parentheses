class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        s_list=list()
        pair={'(':')','{':'}','[':']'}
        right=[')','}',']']
        for char in s:
            if char not in right:
                s_list.append(char)
            else:
                if len(s_list)<=0 or char !=pair.get(s_list.pop()):
                    return False
        if len(s_list)!=0:
            return False
        return True
