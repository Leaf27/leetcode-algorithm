// iterative
class Solution(object):
    def isPowerOfThree(self, n):
        """
        :type n: int
        :rtype: bool
        """
        while(n%3==0 and n>1):
            n/=3
        return n==1
        
// log
import math
def isPowerOfThree(n):
    """
    :type n: int
    :rtype: bool
    """
    result = math.log(n)/math.log(3)

    return result-int(result)==0

// pow(3,19) is the max int which is power of 3 
class Solution(object):
    def isPowerOfThree(self, n):
        """
        :type n: int
        :rtype: bool
        """
        return n>0 and pow(3,19)%n==0
