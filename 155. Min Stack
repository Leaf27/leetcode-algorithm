// using two stack
class MinStack(object):

    def __init__(self):
        """
        initialize your data structure here.
        """
        self.min=list()
        self.mystack=list()
        

    def push(self, x):
        """
        :type x: int
        :rtype: void
        """
        if len(self.min)==0 or x<=self.min[-1]:
            self.min.append(x)
        self.mystack.append(x)
        

    def pop(self):
        """
        :rtype: void
        """
        if len(self.mystack)==0:
            return None
        x=self.mystack.pop()
        if x==self.min[-1]:
            self.min.pop()
  
    def top(self):
        """
        :rtype: int
        """
        if len(self.mystack)==0:
            return None
        return self.mystack[-1]
        

    def getMin(self):
        """
        :rtype: int
        """
        if len(self.min)==0:
            return None
        return self.min[-1]
        


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(x)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
