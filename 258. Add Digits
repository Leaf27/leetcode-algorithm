class Solution(object):
    def addDigits(self, num):
        """
        :type num: int
        :rtype: int
        """
        if num==0:
            return 0
        else:
            return 1+(num-1)%9
            
            
// ditial root(dr) = num%9 ( when num%9 !=0)
                  0r = 9 (when num%9==0 )
                  
