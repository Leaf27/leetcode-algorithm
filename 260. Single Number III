class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        result=[]
        nums.sort()
        i=0
        while i<len(nums):
            if i+1==len(nums) or nums[i]!=nums[i+1]:
                result.append(nums[i])
                i+=1
            else:
                i+=2
        return result
