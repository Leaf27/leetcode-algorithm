public class Solution {
    public void rotate(int[] nums, int k) {
       int length=nums.length;
       k=k%length;
          int[] rotating=new int[k];
          for(int i=0,j=length-k;i<k;i++,j++){
              rotating[i]=nums[j];
          }
          for(int n=length-k-1;n>=0;n--) nums[n+k]=nums[n];
          for(int z=0;z<rotating.length;z++) nums[z]=rotating[z];
    }
}
