class Solution(object):
    def numTrees(self, n):
        """
        :type n: int
        :rtype: int
        """
        dp=[1,1,2]+[0]*(n-2)
        def Trees(n,dp):
            if n==0 or n==1 or n==2:
                return dp[n]
            for i in range(1,n+1):
                if dp[i-1]!=0:
                    left=dp[i-1]
                else:
                    left=Trees(i-1,dp)
                    
                if dp[n-i]!=0:
                    right=dp[n-i]
                else:
                    right=Trees(n-i,dp)
                dp[n]+=left*right
            return dp[n]
        Trees(n,dp)
        return dp[n]
                
